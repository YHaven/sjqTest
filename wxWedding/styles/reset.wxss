@import "../styles/fontic.wxss";
@import "../component/nav/nav.wxss";
@import "../component/message/message.wxss";
.clear {clear:both; height:0; font-size:0; line-height:0; overflow:hidden;}
.cle:after {visibility:hidden; display:block; font-size:0; content:'\20'; clear:both; height:0;}
.clearfix:after{visibility:hidden; display:block; font-size:0; content:'\20'; clear:both; height:0;}
.clear_f:after{visibility:hidden; display:block; font-size:0; content:'\20'; clear:both; height:0;}
.cle_float:after{visibility:hidden; display:block; font-size:0; content:'\20'; clear:both; height:0;}

.f-l{float: left;}
.f-r{float: right;}

.blue-btn{padding: 10rpx;background: #84d9d4;border-radius: 50rpx;margin: 10rpx auto;width:200rpx; }
.red-btn{padding: 10rpx;background: #a6010a;border-radius: 50rpx;margin: 22rpx auto;width:200rpx;color: #f9b61d; }
.text-c{text-align: center}
.text-l{text-align: left}
.text-r{text-align: right}
.m-t20{margin-top: 20rpx;}
.m-t50{margin-top: 50rpx;}
.p-b150{padding-bottom: 150rpx;}

.bc_green{background: green;}
.bc_red{background: red;}
.bc_yellow{background: yellow;}
.bc_blue{background: blue;}


page{background-color: #fff;font-family: "宋体"}
image{width: 100%;height: 100%;}

.loading{text-align: center;color: #ccc;}
.loading-tip {width: 100%;height: 80rpx;line-height: 80rpx;text-align: center;color: #ccc;}
.nav-item.createOrder{background-color: #cd010d;color: #fff;height: 100rpx;line-height: 100rpx;}
.nav-item.createOrder .nav-name{font-size: 36rpx;}
.nav-item.createOrder.disabled{background-color: #999;}

