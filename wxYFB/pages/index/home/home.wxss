@import "../../../styles/reset.wxss";
@import "../../../styles/fontic.wxss";
@import "../../../component/nav/nav.wxss";
.banner{height: 350rpx;}
.banner image{width: 100%;height:100%;}
.order-item{margin-top: 20rpx;background-color: #fff;padding: 30rpx;padding-top:0rpx;border-bottom: 1px solid #e5e5e5;}
.order-item .title{border-bottom: 1px solid #f6f6f6;font-size: 32rpx;height:90rpx;line-height:90rpx;}
.order-item .title .iconfont{color: #999;}
.order-item .title .f-r{color: #a2a2a2;font-size: 24rpx;}
.order-item .info{padding-top: 20rpx;color: #999999;}
.order-item .info .s-name{font-size: 28rpx;margin-bottom: 6rpx;}
.order-item .info .s-count{font-size: 24rpx;}
.order-item .info .s-money{font-size: 26rpx;color: #333;}
.order-item .info .s-money text{font-size: 26rpx;color: #00aaee;}
.order-item .info .s-money text text{font-size: 32rpx;color: #00aaee;}
.order-item .info .s-button{background-color:#00aaee;text-align: center;color: #fff;border-radius: 5rpx;padding:5rpx 8rpx; margin-top: 10rpx;font-size: 26rpx;float: right;width: 130rpx; }
.create-code{text-align: center;margin-top: 100rpx;font-size: 28rpx;margin-bottom:124rpx;}
.create-code image{width: 20rpx;height: 16rpx;}